<aura:component >
  <aura:attribute name="class" type="String" description="CSS classname for the SVG element" />
  <aura:attribute name="xlinkHref" type="String" description="SLDS icon path. Ex: /assets/icons/utility-sprite/svg/symbols.svg#download" />
  <aura:attribute name="svgPath" type="String" description="SLDS icon path. Ex: /assets/icons/utility-sprite/svg/symbols.svg#close" />
  <aura:attribute name="ariaHidden" type="String" default="true" description="aria-hidden true or false. defaults to true" />
  <aura:attribute name="category" type="String" description="category of svg" />
  <aura:attribute name="size" type="String"  description="size of svg" />
  <aura:attribute name="alt" type="String"  description="Loading.." />
  <aura:attribute name="colorCode" type="String" description="Background color"/> 
</aura:component>